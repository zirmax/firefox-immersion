{
  "manifest_version": 2,
  "name": "Immersion",
  "version": "0.1",
  "description": "Adds a red border to all webpages matching mozilla.org.",
  "homepage_url": "https://github.com/zirmax/firefox-immersion",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "content_scripts": [
    {
      "js": ["immersion.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": [
    "dict.json"
  ]
}
